{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"],
            "@components/*": ["./src/components/*"],
            "@external/*": ["./src/external/*"],
            "@hooks/*": ["./src/hooks/*"],
            "@io/*": ["./src/io/*"],
            "@nais/*": ["./src/nais/*"],
            "@observability/*": ["./src/observability/*"],
            "@routes/*": ["./src/routes/*"],
            "@saksbilde/*": ["./src/routes/saksbilde/*"],
            "@oversikt/*": ["./src/routes/oversikt/*"],
            "@spesialist-mock/*": ["./src/spesialist-mock/*"],
            "@state/*": ["./src/state/*"],
            "@apollo-mocks": ["./src/test/tiny-collection-of-mocks"],
            "@test-utils": ["./src/test/test-utils"],
            "@test-wrappers": ["./src/test/wrappers"],
            "@test-data/*": ["./src/test/data/*"],
            "@typer/*": ["./src/types/*"],
            "@utils/*": ["./src/utils/*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
